        <li><%= link_to "Go to parkings list", parkings_path %></li>
        <% if current_person.present? %>
        <li><%= link_to "Go to cars list", cars_path %></li>
        <li><%= link_to "Go to place rents list", place_rents_path %></li>
        <li>Logged in as: <br /><%= current_person.full_name %></li>
