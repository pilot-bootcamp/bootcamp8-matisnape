%h1 Place rent
%table.table.table-striped
  %thead
    %tr
      %th Start date
      %th End date
      %th Parking
      %th Car
      %th Price of place rent
  %tbody
    %tr
      %td= @place_rent.starts_at
      %td= @place_rent.ends_at
      %td= link_to "#{@place_rent.parking.address.zip_code}  #{@place_rent.parking.address.city} #{@place_rent.parking.address.street}", parking_path(@place_rent.parking)
      %td= link_to "#{@place_rent.car.model} #{@place_rent.car.registration_number}", car_path(@place_rent.car)
      %td= @place_rent.price
%br/
= link_to "Go back", place_rents_path
