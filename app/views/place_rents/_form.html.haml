= form_for [@parking, @place_rent], html: { class: "form-horizontal center" } do |f|
  .form-group
    = f.label :car_id, t('place_rents.form.select_car'), class: "col-md-4 control-label"
    .col-md-8
      = f.collection_select(:car_id, current_person.cars, :id, :display_car)
    %br/
  .form-group
    = f.label :starts_at, t('place_rents.form.start_date'), class: "col-md-4 control-label"
    .col-md-8
      = f.datetime_select :starts_at
    %br/
  .form-group
    = f.label :ends_at, t('place_rents.form.end_date'), class: "col-md-4 control-label"
    .col-md-8
      = f.datetime_select :ends_at
    %br/
  .form-group
    .col-md-8.col-md-offset-4
      = f.submit t('buttons.submit'), class: "btn btn-primary"
    %br/
%br/
