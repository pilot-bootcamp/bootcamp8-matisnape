<h1>Place rent</h1>

<table>
  <thead>
    <tr>
      <th>Start date</th>
      <th>End date</th>
      <th>Parking</th>
      <th>Car</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @place_rent.starts_at %></td>
      <td><%= @place_rent.ends_at %></td>
      <td><%= link_to "#{@place_rent.parking.address.zip_code}  #{@place_rent.parking.address.city} #{@place_rent.parking.address.street}", parking_path(@place_rent.parking) %></td>
      <td><%= link_to "#{@place_rent.car.model} #{@place_rent.car.registration_number}", car_path(@place_rent.car) %></td>
    </tr>
  </tbody>
</table><br />

<%= link_to "Go back", place_rents_path %>
