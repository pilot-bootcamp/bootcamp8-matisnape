<%= form_for @car do |f| %>

  <%= f.fields_for :owner_attributes, @car.owner do |owner_fields| %>
    Enter first name: <%= owner_fields.text_field :first_name %><br />
    Enter last name: <%= owner_fields.text_field :last_name %><br />
  <% end %>

  <%= f.label :registration_number, "Enter registration number:" %>
  <%= f.text_field :registration_number %><br />
  <%= f.label :model, "Enter model:" %>
  <%= f.text_field :model %><br />
  <%= f.submit "Submit" %><br />
<% end %><br />
