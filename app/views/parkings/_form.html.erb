<%= render 'shared/error_messages', object: @parking %>


  <%= form_for @parking, html: { class: "form-horizontal center" } do |f| %>
    <%= f.fields_for :address_attributes, @parking.address do |address_fields| %>
    <div class="form-group">
      <%= address_fields.label :city, "Enter city:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= address_fields.text_field :city %><br />
      </div>
    </div>
    <div class="form-group">
      <%= address_fields.label :street, "Enter street:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= address_fields.text_field :street %><br />
      </div>
    </div>
    <div class="form-group">
      <%= address_fields.label :zip_code, "Enter zipcode:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= address_fields.text_field :zip_code %><br />
      </div>
    </div>
    <% end %>
    <div class="form-group">
      <%= f.label :places, "Enter number of places available:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= f.text_field :places %><br />
      </div>
    </div>
    <div class="form-group">
      <%= f.label :hour_price, "Enter price per hour:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= f.text_field :hour_price %><br />
      </div>
    </div>
    <div class="form-group">
      <%= f.label :day_price, "Enter price per day:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= f.text_field :day_price %><br />
      </div>
    </div>
    <div class="form-group">
      <%= f.label :kind, "Enter kind:", class: "col-md-4 control-label" %>
      <div class="col-md-8">
        <%= f.select :kind do %>
          <%= options_for_select %w(outdoor indoor private street), @parking.kind %>
        <% end %><br />
      </div>
    </div>
    <div class="form-group">
      <%= f.submit "Submit", class: "btn btn-primary" %><br />
    </div>
  <% end %><br />
