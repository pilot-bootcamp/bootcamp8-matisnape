<%= render 'shared/error_messages', object: @parking %>


  <%= form_for @parking, html: { :class => "form-horizontal center" } do |f| %>
    <%= f.fields_for :address_attributes, @parking.address do |address_fields| %>
      <%= address_fields.label :city, "Enter city:" %>
      <%= address_fields.text_field :city %><br />
      <%= address_fields.label :street, "Enter street:" %>
      <%= address_fields.text_field :street %><br />
      <%= address_fields.label :zip_code, "Enter zipcode:" %>
      <%= address_fields.text_field :zip_code %><br />
    <% end %>

    <%= f.label :places, "Enter number of places available:" %>
    <%= f.text_field :places %><br />
    <%= f.label :hour_price, "Enter price per hour:" %>
    <%= f.text_field :hour_price %><br />
    <%= f.label :day_price, "Enter price per day:" %>
    <%= f.text_field :day_price %><br />
    <%= f.label :kind, "Enter kind:" %>
    <%= f.select :kind do %>
      <%= options_for_select %w(outdoor indoor private street), @parking.kind %>
    <% end %><br />
    <%= f.submit "Submit", class: "btn btn-primary" %><br />
  <% end %><br />
